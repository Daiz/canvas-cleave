{
  "rules": {
    "await-promise": true,
    "triple-equals": [true, "allow-null-check"],
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "member-variable-declaration"
    ],
    "unified-signatures": true
  },
  "extends": ["tslint-config-prettier", "tslint-no-circular-imports"]
}
