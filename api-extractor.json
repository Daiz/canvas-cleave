{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/api-extractor.schema.json",
  "compiler": {
    "configType": "tsconfig",
    "rootFolder": "."
  },
  "project": {
    "entryPointSourceFile": "lib/index.d.ts"
  },
  "apiReviewFile": {
    "enabled": true,
    "apiReviewFolder": "config"
  },
  "apiJsonFile": {
    "enabled": true,
    "outputFolder": "config"
  },
  "dtsRollup": {
    "enabled": true,
    "trimming": false
  }
}
