{
  "name": "canvas-cleave",
  "version": "0.0.0",
  "description": "100% JS Node.js shim for HTML5 Canvas drawImage (without resizing)",
  "main": "dist/canvas-cleave.min.js",
  "typings": "dist/canvas-cleave.d.ts",
  "tsdoc": {
    "tsdocFlavor": "AEDoc"
  },
  "module": "lib/index.js",
  "scripts": {
    "test": "jest",
    "start": "jest --watch",
    "build:lib": "rimraf lib && tsc --build config/tsconfig.lib.json",
    "build:dist": "rimraf dist && webpack && api-extractor run --config config/api-extractor.dist.json",
    "build": "npm run build:lib && npm run build:dist",
    "coverage": "jest --coverage",
    "api-extractor": "npm run build:lib && api-extractor run --local"
  },
  "author": "Daiz <taneli.vatanen@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "^7.1.0",
    "@types/jest": "^24.0.11",
    "@types/sharp": "^0.21.3",
    "awesome-typescript-loader": "^5.2.1",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "sharp": "^0.21.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-circular-imports": "^0.6.2",
    "typescript": "^3.4.4",
    "typescript-tslint-plugin": "^0.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  }
}
